<?php
class Calculator {

function sum($x, $y) {
	return $x + $y;
}

function sub($x, $y) {
	return $x - $y;
}

function mul($x, $y) {
	return $x * $y;
}

function div($x, $y) {
	return $x / $y;
}
}

$calculator = new Calculator();
echo $calculator->sum(5, 7); echo "\n";
echo $calculator->sub(8, 2); echo "\n";
echo $calculator->mul(2, 5); echo "\n";
echo $calculator->div(20, 4); echo "\n";
?>
